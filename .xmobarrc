Config { font = "-*-terminus-*-*-*-*-12-*-*-*-*-*-*-u"
       , bgColor = "#000000"
       , fgColor = "#C9A34E"
       , border = NoBorder
       , borderColor = "#000000"
       , position = Static { xpos = 0, ypos = 0, width = 1166, height = 26 }
       , lowerOnStart = True
       , hideOnStart = False
       , persistent = True
       , commands = [ Run Network "wlp3s0" ["-L", "8", "-H", "32", "-l", "#C9A34E", "-n", "#429942" ,"-h", "#A36666", "-t", "<dev>: <rx> : <tx>"] 10
                    , Run Cpu ["-L", "3", "-H", "50", "--normal", "#429942", "--high", "#A36666"] 10
                    , Run Memory ["-t", "Mem: <usedratio>%"] 10
    		    , Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
                    , Run CoreTemp ["-t", "Temp: <core0>C : <core1>C : <core2>C : <core3>C"] 10
                    , Run Battery ["-t", "Batt: <acstatus> (<left>% / <timeleft>)", "--", "-f", "ADP1/online", "-l", "red", "-m", "blue", "-h", "green"] 600
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = " %StdinReader% }{ %battery% <fc=#429942>|</fc> %cpu% <fc=#429942>|</fc> %memory% <fc=#429942>|</fc> %wlp3s0% <fc=#429942>|</fc> %date%"
       }
